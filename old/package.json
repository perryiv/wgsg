{
  "name": "wgsg",
  "description": "WebGPU Scene Graph",
  "license": "MIT",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build-lib": "cd lib && yarn run build",
    "clear": "clear"
  },
  "save": {
    "compile": "tsc --build",
    "dev": "vite",
    "http": "http-server -c-1 --cors",
    "lint": "eslint src/lib src/app",
    "preview": "vite preview",
    "test": "mocha src/tests/main.js",
    "watch": "watch \"yarn run clear && yarn run build && yarn run lint && yarn run test && echo watching ...\" src"
  },
  "mocha": {
    "node-option": [
      "import=tsx"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/chai": "^5.2.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.3.4",
    "@webgpu/types": "^0.1.56",
    "chai": "^4.2.0",
    "clear-cli": "^2.0.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "http-server": "^14.1.1",
    "mocha": "^11.1.0",
    "puppeteer-core": "^24.4.0",
    "tsx": "^4.19.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "watch": "^1.0.2"
  }
